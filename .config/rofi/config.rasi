configuration {
    modi: "drun,run,ssh";
    drun-display-format: "{icon} {name}";
    show-icons:         true;
    icon-theme:         "Papirus";
	location: 0;
	yoffset: 0;
	xoffset: 0;
	fullscreen: false;
	terminal: "urxvt";
	columns: 3;
	sidebar-mode: false;
}
* {
  background-color: rgba(0, 0, 0, 0);
  font: "SF Mono 11";
  text-color: #6c605a;
}
window {
  padding: 20px;
  background-color: #e6dad6;
}
